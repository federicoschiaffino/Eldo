    "def iter_clean(ll):\n",
    "    res = []\n",
    "    for l in ll:\n",
    "        if type(l) == tuple:\n",
    "            res.append(l)\n",
    "        else:\n",
    "            res_iter = iter_clean(l)\n",
    "            res += res_iter\n",
    "    return res\n",
    "\n",
    "\n",
    "def remove_duplicates(lst):    \n",
    "    ll = iter_clean(lst)\n",
    "    return [(a, b) for i, [a, b] in enumerate(ll) if not any(c == b for _, c in ll[:i])]\n",
    "\n",
    "\n",
    "def final_clean(ll):\n",
    "    res = []\n",
